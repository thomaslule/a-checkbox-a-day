{
  "name": "a-checkbox-a-day",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomaslule/a-checkbox-a-day.git"
  },
  "scripts": {
    "start": "node_modules/.bin/nodemon -e js index.js",
    "test": "npm run unit && npm run e2e",
    "install_selenium": "node_modules/.bin/webdriver-manager update --standalone",
    "start_selenium": "node_modules/.bin/webdriver-manager start",
    "selenium": "npm run install_selenium && npm run start_selenium",
    "unit": "karma start ./test/unit/karma_conf.js --single-run",
    "e2e": "node_modules/.bin/protractor ./test/e2e/protractor_conf.js"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.12.0",
    "express": "^4.11.2",
    "fs-extra": "^0.16.3",
    "moment": "^2.9.0",
    "mysql": "^2.5.4",
    "nconf": "^0.7.1",
    "nodemon": "^1.3.7",
    "retry": "^0.6.1"
  },
  "devDependencies": {
    "angular": "^1.4.7",
    "angular-mocks": "^1.4.7",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "protractor": "^2.5.1"
  }
}
